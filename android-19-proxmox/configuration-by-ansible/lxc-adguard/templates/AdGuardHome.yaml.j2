bind_host: {{ adguard_web_bind }}
bind_port: {{ adguard_web_port }}
users:
  - name: {{ adguard_admin_username }}
    password: $2a$10$Tk5.5d8/i/6PoAP4LmVVvO4qDCtqoOGh1hzn5XyGBp5aqrmKdZT4y  # bcrypt hash of 'homelab123'
auth_attempts: 5
block_auth_min: 15
http_proxy: ""
language: en
debug_pprof: false
web_session_ttl: 720
dns:
  bind_hosts:
    - {{ adguard_dns_bind }}
  port: {{ adguard_dns_port }}
  statistics_interval: {{ adguard_statistics_interval }}
  querylog_enabled: {{ adguard_querylog_enabled | lower }}
  querylog_file_enabled: true
  querylog_interval: {{ adguard_querylog_interval }}
  querylog_size_memory: 1000
  anonymize_client_ip: false
  protection_enabled: {{ adguard_filtering_enabled | lower }}
  blocking_mode: default
  blocking_ipv4: ""
  blocking_ipv6: ""
  blocked_response_ttl: {{ adguard_blocked_response_ttl }}
  parental_block_host: family-block.dns.adguard.com
  safebrowsing_block_host: standard-block.dns.adguard.com
  ratelimit: 20
  ratelimit_whitelist: []
  refuse_any: true
  upstream_dns: {{ adguard_upstream_dns | to_yaml }}
  upstream_dns_file: ""
  bootstrap_dns: {{ adguard_bootstrap_dns | to_yaml }}
  all_servers: false
  fastest_addr: false
  fastest_timeout: 1s
  allowed_clients: []
  disallowed_clients: []
  blocked_hosts: {{ adguard_custom_filtering_rules | to_yaml }}
  cache_size: {{ adguard_cache_size }}
  cache_ttl_min: {{ adguard_cache_ttl_min }}
  cache_ttl_max: {{ adguard_cache_ttl_max }}
  cache_optimistic: false
  bogus_nxdomain: []
  aaaa_disabled: false
  enable_dnssec: false
  edns_client_subnet:
    custom_ip: ""
    enabled: false
    use_custom: false
  max_goroutines: 300
  handle_ddr: true
  ipset: []
  ipset_file: ""
  filtering_enabled: {{ adguard_filtering_enabled | lower }}
  filters_update_interval: 24
  parental_enabled: {{ adguard_parental_enabled | lower }}
  safesearch_enabled: {{ adguard_safesearch_enabled | lower }}
  safebrowsing_enabled: {{ adguard_safebrowsing_enabled | lower }}
  safebrowsing_cache_size: 1048576
  safesearch_cache_size: 1048576
  parental_cache_size: 1048576
  cache_time: 30
  rewrites: {{ adguard_dns_rewrites | to_yaml }}
  blocked_services: []
  upstream_timeout: 10s
  private_networks: []
  use_private_ptr_resolvers: true
  local_ptr_upstreams: []
  use_dns64: false
  dns64_prefixes: []
  serve_http3: false
  use_http3_upstreams: false
tls:
  enabled: false
  server_name: ""
  force_https: false
  port_https: 443
  port_dns_over_tls: 853
  port_dns_over_quic: 784
  port_dnscrypt: 0
  dnscrypt_config_file: ""
  allow_unencrypted_doh: false
  certificate_chain: ""
  private_key: ""
  certificate_path: ""
  private_key_path: ""
  strict_sni_check: false
filters:
{% for filter in adguard_filter_lists %}
{% if filter.enabled | default(true) %}
  - enabled: true
    url: {{ filter.url }}
    name: {{ filter.name }}
    id: {{ loop.index }}
{% endif %}
{% endfor %}
whitelist_filters: []
user_rules: {{ adguard_custom_filtering_rules | to_yaml }}
dhcp:
  enabled: false
  interface_name: ""
  local_domain_name: lan
  dhcpv4:
    gateway_ip: ""
    subnet_mask: ""
    range_start: ""
    range_end: ""
    lease_duration: 86400
    icmp_timeout_msec: 1000
    options: []
  dhcpv6:
    range_start: ""
    lease_duration: 86400
    ra_slaac_only: false
    ra_allow_slaac: false
clients:
  runtime_sources:
    whois: true
    arp: true
    rdns: true
    dhcp: true
    hosts: true
  persistent: []
log_compress: false
log_localtime: false
log_max_backups: 0
log_max_size: 100
log_max_age: 3
log_file: ""
verbose: false
os:
  group: ""
  user: ""
  rlimit_nofile: 0
schema_version: 28